{
    "name": "qortex/graph-sdk",
    "description": "Facebook SDK with PHP 8 support",
    "keywords": ["facebook", "sdk"],
    "type": "library",
    "homepage": "https://github.com/QortexDevs/php-graph-sdk",
    "license": "Facebook Platform",
    "authors": [
        {
            "name": "Facebook",
            "homepage": "https://github.com/facebook/php-graph-sdk/contributors"
        }
    ],
    "require": {
        "php": "^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "guzzlehttp/guzzle": "^7.2"
    },
    "suggest": {
        "paragonie/random_compat": "Provides a better CSPRNG option in PHP 5",
        "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
    },
    "autoload": {
        "psr-4": {
            "Facebook\\": "src/Facebook/"
        },
        "files": ["src/Facebook/polyfills.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Facebook\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    }
}
